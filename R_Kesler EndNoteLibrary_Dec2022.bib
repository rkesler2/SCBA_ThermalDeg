}

@article{harvey_respiratory_2008,
	title = {Respiratory gas exchange and physiological demands during a fire fighter evaluation circuit in men and women},
	volume = {103},
	issn = {1439-6319 (Print) 1439-6319 (Linking)},
	doi = {10.1007/s00421-008-0673-2},
	abstract = {We examined the oxygen uptake (VO2) and carbon dioxide output (VCO2) during completion of a circuit developed for testing fire fighters and related performance time to laboratory measures of fitness. Twenty-two healthy university students (ten women) were trained in the tasks then performed the circuit as quickly as possible. Breath-by-breath gas exchange and heart rate were continuously measured with a portable system. Median circuit time was 6:13 (min:s, 25-75\% = 5:46-6:42) for men and 7:25 (25-75\% = 6:49-10:21) for 8 women finishers (P = 0.023), and VO2 averaged 68 and 64\% VO2max for the men and women during the circuit. Both men and women had high respiratory exchange ratios ({\textgreater}1.0) suggesting marked anaerobic energy contribution. Physiological variables associated with circuit time were assessed by backward stepwise regression yielding a significant model that included only peak work rate during arm cranking exercise as a function of circuit completion time across men and women combined (P {\textless} 0.001). For men, but especially for women, the time required for the simulated victim drag (68.2 kg mannequin) was positively correlated with total time to complete the other circuit elements (r = 0.51, r = 0.96 respectively). The simple correlation between circuit time and VO2max (mL/kg/min) revealed poor relationships for men (r = -0.37, P {\textgreater} 0.05) and women (r = 0.20, P {\textgreater} 0.05). These data demonstrated that upper body fitness as reflected by peak work rate during arm cranking correlated with total circuit time for the men and women in our population sample.},
	language = {eng},
	number = {1},
	journal = {Eur J Appl Physiol},
	author = {Harvey, D. G. and Kraemer, J. L. and Sharratt, M. T. and Hughson, R. L.},
	month = may,
	year = {2008},
	note = {Edition: 2008/01/22},
	keywords = {Humans, Adult, Heart Rate/physiology, Male, Physical Endurance/physiology, Female, Oxygen Consumption/ physiology, Regression Analysis, Fires, Predictive Value of Tests, Occupational Health, Physical Fitness/ physiology, Energy Metabolism/physiology, Models, Biological, Pulmonary Gas Exchange/ physiology},
	pages = {89--98},
}

