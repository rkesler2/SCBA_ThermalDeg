	keywords = {Humans, Adult, Male, Physical Endurance/physiology, Adolescent, Female, Risk Factors, Reference Values, Young Adult, Exercise Test, Heart Rate, Postural Balance/physiology, Time Factors, Posture/physiology, Anaerobic Threshold/physiology, United States/epidemiology, Exercise Tolerance/physiology, Running/physiology, Muscle Fatigue/physiology, Exercise/physiology, Sports},
	pages = {456--63},
}

