	note = {Edition: 2013/04/06},
	keywords = {Humans, Adult, Male, Physical Endurance/physiology, *Exercise, Gait/*physiology, Young Adult, Biomechanical Phenomena, Fatigue, Kinetics, Motor Activity/*physiology, Motor control, Motor Skills/*physiology, Muscle Fatigue/*physiology, Muscle, Skeletal/*physiology, Obstacle, Physical activity, Sedentary Lifestyle, Walking},
	pages = {702--7},
}

