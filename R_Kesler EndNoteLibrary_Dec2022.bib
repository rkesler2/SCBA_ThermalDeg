	keywords = {Humans, Adult, Male, Female, Firefighters, Young Adult, Equipment Design, *Human Engineering, Fires/*prevention \& control, *Lifting, Accelerometry, Consumer Behavior, ergonomics, fire fighting equipment, Load carriage, Materials Testing, performance assessment},
