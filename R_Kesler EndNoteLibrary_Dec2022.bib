@article{szmytke_firefighters_2022,
	title = {Firefighters' {Clothing} {Contamination} in {Fires} of {Electric} {Vehicle} {Batteries} and {Photovoltaic} {Modules}-{Literature} {Review} and {Pilot} {Tests} {Results}},
	volume = {19},
	issn = {1661-7827 (Print) 1660-4601},
	doi = {10.3390/ijerph191912442},
	abstract = {The electric vehicle (EV) market, together with photovoltaic (PV) installations continues to develop at a pace. However, there are concerns that EV and PV installation fires may create more harmful substances than other types of fire. PV modules and car battery fires emit a range of carcinogenic and highly toxic compounds that are not yet fully understood and may pose a threat to firefighters' health. This also raises the question of the impact on firefighters' clothing and the safe handling and cleaning after such fires. This article presents a literature and standards review of the firefighters' protective clothing maintenance and cleaning. It also contains test results showing that firefighters' clothes accumulate harmful substances after fighting these types of fires. Pilot tests for the presence of polycyclic aromatic hydrocarbons (PAHs) and formaldehyde showed that levels exceeded limits in all clothing samples. For example, the cobalt level was 24 times higher than that considered safe in the test carried out with car battery fire. Although it is recognized that liquid carbon dioxide (LCO(2)) methods of cleaning may be more effective than traditional water washing, further research on cleaning efficiency for clothing containing substances emitted from car battery and PV modules fires is required.},
	language = {eng},
	number = {19},
	journal = {Int J Environ Res Public Health},
	author = {Szmytke, E. and Brzezińska, D. and Machnowski, W. and Kokot, S.},
	month = sep,
	year = {2022},
	note = {Edition: 2022/10/15},
	keywords = {Humans, *Fires, *Firefighters, Protective Clothing, firefighters, firefighter safety, *Air Pollutants, Occupational/analysis, *Occupational Exposure/analysis, *Polycyclic Aromatic Hydrocarbons/analysis, car battery, Carbon Dioxide, Cobalt, electric vehicle, electrical fire, financed by Decontex Holding based in Belgium. Nevertheless the analysis and, firefighters’ clothing cleaning, firefighters’ protective clothing, Formaldehyde, interpretation of the data were solely the work of the authors and was not in any, liquid CO2 firefighters’ clothing decontamination, PV fire, The tests have been carried out in Application Lab WEBER \& LEUCHT GMBH and, Water, way influenced by the external collaborators. This work was not paid for.},
