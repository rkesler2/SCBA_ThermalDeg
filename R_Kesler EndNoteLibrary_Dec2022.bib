@article{hocke_comparative_1997,
	title = {Comparative evaluation of the characteristics of fire protection jackets worn by fire fighters ({Phase} {II})},
	volume = {32},
	shorttitle = {Vergleichende bewertung des trageverhaltens von feuerwehr-einsatzjacken ({Phase} {II})},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0030763732&partnerID=40&md5=3bfd1b15f60d24bf74c601a864dba787},
	abstract = {Four different fire protection jackets, five different sets of underwear (four functional and one cotton) and two compressed-air breathing devices of different weights which are independent of the ambient air were investigated comparatively under practice-oriented standardized laboratory conditions (climate chamber, treadmill) by four specially chosen and trained test subjects. As in phase I, the practical orientation was reflected in parameters climate, heaviness of work and use of breathing apparatus. The aim of phase II was to find out in a defined time and physical strain regime (submaximal strain) how the four fire protection jackets to be tested, the underwear and the compressed-air breathing apparatus perform with regard to the parameters heart rate, core body temperature, changes of microclimate, weight changes of subjects and clothing, and perceived comfort. On the basis of the above parameters, especially the skin temperature and skin moisture curves which are the most informative with regard to clothing physiology, the following ranking of the jackets was found: Mc Bean, Texport, Bristol and Teled. As expected, the functional underwear could not ranked on the basis of the experiments carried out by only one person. However, noticeable drying effects were shown in the person wearing the functional underwear. These facts are of course not manifested with cotton underwear. Combination of functional underwear and a protective jacket permeable to water vapor using a functional shirt as a substitute for the cotton shirt typical for uniforms can be recommended. This enables a functional interaction of the system as a whole. Reducing the weight of the compressed-air breathing apparatus had a positive effect on the exercise-dependent heart rate.},
	number = {8},
	journal = {Arbeitsmedizin Sozialmedizin Umweltmedizin},
	author = {Hocke, M. and Schopper-Jochum, S. and Schubert, W.},
	year = {1997},
	keywords = {Breathing apparatus, Clothing physiology, Fire protective jackets (DIN EN 469), Functional underwear, Practice orientated test conditions, Wear comfort},
	pages = {314--320},
}

@article{holewijn_influence_1992,
	title = {The influence of backpack design on physical performance},
	volume = {35},
	issn = {0014-0139},
	url = {http://dx.doi.org/10.1080/00140139208967803},
	doi = {10.1080/00140139208967803},
	abstract = {In an experiment with 10 different backpack configurations, the loss of physical performance due to weight and volume, restriction of shoulder motion, and interference with balance were quantified on a test battery. Ten well-trained infantry soldiers were subjected to several tests, including an obstacle course, jumping, sprinting, running, hand-grenade throwing, and a mobility test. On the test battery average losses of performance of 1\% per kg mass and 0·2\% per litre backpack volume were found. The effects of weight and volume on the physical performance were additive. Motion restriction of the shoulders did not result in significant performance losses. Balance disturbance resulted in a 1·5\% performance decrement. In order to minimize the loss of physical performance, weight should be centred around the waist. Volume may be distributed over the chest and the back without extra performance decrements.},
	number = {2},
	urldate = {2014-10-07},
	journal = {Ergonomics},
	author = {Holewijn, M. and Lotens, W. A.},
	month = feb,
	year = {1992},
	pages = {149--157},
}

@article{holmer_classification_2007,
	title = {Classification of metabolic and respiratory demands in fire fighting activity with extreme workloads},
	volume = {38},
	issn = {0003-6870 (Print) 0003-6870 (Linking)},
	doi = {10.1016/j.apergo.2006.01.004},
	abstract = {Fire fighting work comprises work tasks requiring an energy yield at maximal or close to maximal levels of the individual. Due to the very nature of fire fighting more complex physiological variables are difficult to measure. We measured metabolic and respiratory responses in 15 male, professional fire fighters during simulated work tasks on a test ground. Work time was on the average 22 min with individual components of work tasks lasting 2-4 min. The mean oxygen consumption for the whole exercise (22 min) was 2.75+/-0.29 l/min. The most demanding work task demanded an oxygen uptake of 3.55+/-0.27 l/min. Corresponding values for respiratory minute volumes were 82+/-14 and 102+/-14l/min, respectively. Heart rates averaged 168+/-12 for the whole test and 179+/-13 beats/min for the heaviest work task. Two new classes for classification of intensive and exhausting, short term physical work are proposed for inclusion in ISO8996 and values for relevant parameters are proposed.},
	language = {eng},
	number = {1},
	journal = {Appl Ergon},
	author = {Holmer, I. and Gavhed, D.},
	month = jan,
	year = {2007},
	note = {Edition: 2006/03/07},
	keywords = {Humans, Adult, Male, Heart Rate, Oxygen Consumption, Fires, Task Performance and Analysis, Occupational Health, Physical Exertion/ physiology, Energy Metabolism/ physiology, Respiratory Physiological Phenomena, Pulmonary Ventilation, Workload/ classification},
	pages = {45--52},
}

@article{holmer_test_2006,
	title = {Test of firefighter's turnout gear in hot and humid air exposure},
	volume = {12},
	issn = {1080-3548 (Print) 1080-3548 (Linking)},
	abstract = {Five students of a rescue training school cycled at 50 W for 20 min at 20 degrees C before walking at 5 km/hr up to 30 min in a climatic chamber at 55 degrees C and 30\% relative humidity. 4 different types of clothing ensembles differing in terms of thickness and thermal insulation value were tested on separate days. All subjects completed 28-30 min in light clothing, but quit after 20-27 min in 3 firefighter ensembles due to a rectal temperature of 39.0 degrees C or subjective fatigue. No difference in the evolution of mean skin or rectal temperature was seen for the 3 turnout ensembles. Sweat production amounted to about 1000 g in the turnout gears of which less than 20\% evaporated. It was concluded that the small differences between the turnout gears in terms of design, thickness and insulation value had no effect on the resulting heat physiological strain for the given experimental conditions.},
	language = {eng},
	number = {3},
	journal = {Int J Occup Saf Ergon},
	author = {Holmer, I. and Kuklane, K. and Gao, C.},
	year = {2006},
	note = {Edition: 2006/09/21},
	keywords = {Humans, Adult, Male, Body Temperature, Fires, Rescue Work, Occupational Health, Fatigue/prevention \& control, Heat Stress Disorders/prevention \& control, Hot Temperature/ adverse effects, Humidity/ adverse effects, Protective Clothing/ standards, Sweat},
	pages = {297--305},
}

@article{hong_hazardous_2007,
	title = {Hazardous decibels: hearing health of firefighters},
	volume = {55},
