@article{hydbring_exosomal_2018,
	title = {Exosomal {RNA}-profiling of pleural effusions identifies adenocarcinoma patients through elevated {miR}-200 and {LCN2} expression},
	volume = {124},
	issn = {0169-5002},
	doi = {10.1016/j.lungcan.2018.07.018},
	abstract = {HYPOTHESIS: The inherent challenges associated with tissue biopsies from lung have spurred an interest in the use of liquid biopsies. Pleural effusions are one source of liquid biopsy. Recently, extracellular vesicles of endocytic origin, exosomes, have attracted interest as liquid biopsy of tumors as they are thought to be a mirror of their tumor of origin. Here, we aimed to analyze if RNA profiling of exosomes isolated from pleural effusions could differentiate patients with lung adenocarcinoma from patients with benign inflammatory processes. METHODS: Exosomes were isolated from 36 pleural effusions from patients with adenocarcinoma (n = 18) and patients with benign inflammatory processes (n = 18). The two groups were balanced with respect to age and smoking history but with a gender bias towards males in the benign group. Profiling was conducted using RT-qPCR arrays covering 754 microRNAs and 624 mRNAs followed by statistical ranking of differentially regulated transcripts between the two patient cohorts. RESULTS: RNA profiling revealed differential expression of 17 microRNAs and 71 mRNAs in pleural effusions collected from patients with lung adenocarcinoma compared to pleural effusions from benign lung disease. Overall, top differentially expressed microRNAs, including miR-200 family microRNAs, provided a stronger diagnostic power compared to top differentially expressed mRNAs. However, the mRNA transcript encoding Lipocalin-2 (LCN2) displayed the strongest diagnostic power of all analyzed transcripts (AUC: 0.9916). CONCLUSIONS: Our study demonstrates that exosomal RNA profiling from pleural effusions can be used to identify patients with lung adenocarcinoma from individuals with benign processes and further proposes miR-200 microRNAs and LCN2 as diagnostic markers in lung cancer liquid biopsies.},
	language = {eng},
	journal = {Lung Cancer},
	author = {Hydbring, P. and De Petris, L. and Zhang, Y. and Brandén, E. and Koyi, H. and Novak, M. and Kanter, L. and Hååg, P. and Hurley, J. and Tadigotla, V. and Zhu, B. and Skog, J. and Viktorsson, K. and Ekman, S. and Lewensohn, R.},
	month = oct,
	year = {2018},
	note = {Edition: 20180727},
	keywords = {Humans, Adult, Male, Female, Aged, Aged, 80 and over, Middle Aged, Gene Expression Profiling, *Exosomes, MicroRNAs/*genetics, Neoplasm Staging, *Lung adenocarcinomas, *microRNAs, *mRNAs, *Pleural effusions, Adenocarcinoma/*diagnosis, Diagnosis, Differential, Exosome Multienzyme Ribonuclease Complex/*genetics, Lipocalin-2/*genetics/metabolism, Liquid Biopsy, Lung Neoplasms/*diagnosis, Pleural Effusion/*genetics, Pneumonia/*diagnosis},
	pages = {45--52},
}

