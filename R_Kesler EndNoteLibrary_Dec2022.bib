	keywords = {Humans, Occupational Exposure/*analysis, *Firefighters, Cytokines, *Inflammation, *Wildfires, Granulocyte-Macrophage Colony-Stimulating Factor, Interferon-gamma, Interleukin-10, Interleukin-1beta, Tumor Necrosis Factor-alpha},
