	keywords = {Humans, Male, Middle Aged, Prognosis, Time Factors, Follow-Up Studies, Retrospective Studies, Genetic Predisposition to Disease, Obesity/*genetics/metabolism, *Polymorphism, Genetic, Alleles, Blotting, Southern, Diabetes Mellitus/*genetics/metabolism, DNA/*genetics, Gene Frequency, Genotype, Hypertension/*genetics/metabolism, Polymerase Chain Reaction, Receptors, Glucocorticoid/*genetics/metabolism},
