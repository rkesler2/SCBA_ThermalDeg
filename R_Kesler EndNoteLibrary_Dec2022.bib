@article{motl_neurological_2014,
	title = {Neurological disability and its association with walking impairment in multiple sclerosis: brief review},
	volume = {4},
	issn = {1758-2024},
	doi = {10.2217/nmt.14.32},
	abstract = {Neurological disability and walking impairment are two common, co-varying consequences of multiple sclerosis (MS) that can result in substantial patient burden for daily activities and quality of life. Indeed, neurological disability and walking impairment are driven by pathological changes in the central nervous system, and measurement of walking function is a common method of monitoring the progression of disease and neurological disability. The existing data indicate that the presence of walking impairments range from performance through real-world outcomes based on comparison of MS versus healthy controls, and walking impairments become worse with increasing severity of neurological disability (i.e., progression). Accordingly, researchers and clinicians have considered both pharmaceutical and rehabilitation approaches for managing walking impairment in MS. Both approaches yield beneficial effects on walking outcomes, although the majority of research has focused on exercise training rather than pharmaceutical interventions. Overall, this underscores the importance of continued efforts toward identifying approaches for preventing, forestalling and restoring walking function in persons with MS across the spectrum of neurological disability and its progression.},
	language = {Eng},
	number = {6},
	journal = {Neurodegener Dis Manag},
	author = {Motl, R. W. and Learmonth, Y. C.},
	year = {2014},
	note = {Edition: 2014/12/23},
	keywords = {Humans, Male, Female, Disability Evaluation, Exercise Test, Walking/*physiology, Disease Progression, ambulation, Cost of Illness, Gait Disorders, Neurologic/*complications/*diagnosis/physiopathology, multiple sclerosis, Multiple Sclerosis/*complications/*diagnosis/physiopathology, neurological disability, walking},
