	keywords = {Humans, Adult, Exercise/*physiology, Male, Linear Models, Exercise Test, Pulmonary Gas Exchange/physiology, Time Factors, Oxygen/*analysis, Exercise Tolerance/physiology, *Pulmonary Ventilation, Carbon Dioxide/*analysis, Respiratory Function Tests/*instrumentation/standards, Running/physiology},
